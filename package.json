{
  "name": "unplugin-yaml",
  "version": "0.1.1",
  "description": "Allow importing YAML files as JSON or Strings",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/unplugin-yaml.git"
  },
  "bugs": "https://github.com/luxass/unplugin-yaml/issues",
  "keywords": [
    "yaml",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "nuxt"
  ],
  "workspaces": [
    "playground/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./esbuild": {
      "import": {
        "types": "./dist/esbuild.d.ts",
        "default": "./dist/esbuild.mjs"
      },
      "require": {
        "types": "./dist/esbuild.d.cts",
        "default": "./dist/esbuild.cjs"
      }
    },
    "./nuxt": {
      "import": {
        "types": "./dist/nuxt.d.ts",
        "default": "./dist/nuxt.mjs"
      },
      "require": {
        "types": "./dist/nuxt.d.cts",
        "default": "./dist/nuxt.cjs"
      }
    },
    "./rollup": {
      "import": {
        "types": "./dist/rollup.d.ts",
        "default": "./dist/rollup.mjs"
      },
      "require": {
        "types": "./dist/rollup.d.cts",
        "default": "./dist/rollup.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.mjs"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./webpack": {
      "import": {
        "types": "./dist/webpack.d.ts",
        "default": "./dist/webpack.mjs"
      },
      "require": {
        "types": "./dist/webpack.d.cts",
        "default": "./dist/webpack.cjs"
      }
    },
    "./types": {
      "types": "./yaml.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "yaml.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "js-yaml": "^4.1.0",
    "unplugin": "^1.10.2"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.7.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.9",
    "esbuild": "^0.21.5",
    "eslint": "^9.6.0",
    "rollup": "^4.18.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.2",
    "vite-plugin-inspect": "^0.8.4",
    "vitest": "1.6.0",
    "webpack": "^5.92.1"
  }
}
